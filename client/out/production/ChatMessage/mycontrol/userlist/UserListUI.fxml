<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="FlowPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="headImageView" fitHeight="50" fitWidth="50" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="nameLabel" text="Label">
         <font>
            <Font name="Microsoft YaHei Bold" size="20.0" />
         </font></Label>
      <Pane prefHeight="20.0" prefWidth="30.0" />
      <JFXButton fx:id="messageNumber" buttonType="RAISED" onAction="#closeMessageNumber" prefHeight="20.0" prefWidth="20.0" style="-fx-background-radius: 90px; -fx-background-color: red;" text="0" textFill="WHITE" visible="false">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </JFXButton>
   </children>
</fx:root>
