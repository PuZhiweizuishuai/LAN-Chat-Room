<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="signInStage" prefHeight="540.0" prefWidth="960.0" style="-fx-background-color: #7f7cf8;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChatMessage.SignUp.SignUpControl">
    <children>
        <Pane layoutX="55.0" layoutY="45.0" prefHeight="450.0" prefWidth="850.0" style="-fx-background-color: white; -fx-background-radius: 10px;">
            <children>
                <ImageView fitHeight="373.0" fitWidth="373.0" layoutX="52.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/LoginBg.png" />
                    </image>
                </ImageView>
            <TextField fx:id="userEmail" layoutX="490.0" layoutY="79.0" prefHeight="50.0" prefWidth="320.0" promptText="邮箱" style="-fx-background-radius: 30px; -fx-background-color: #ebeef1;">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
               <font>
                  <Font name="Microsoft YaHei Bold" size="18.0" />
               </font>
            </TextField>
                <TextField fx:id="userName" layoutX="490.0" layoutY="165.0" prefHeight="50.0" prefWidth="320.0" promptText="用户名" style="-fx-background-radius: 30px; -fx-background-color: #ebeef1;">
                    <font>
                        <Font name="Microsoft YaHei Bold" size="18.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="TEXT" />
                    </cursor>
                </TextField>
                <PasswordField fx:id="userPassword" layoutX="490.0" layoutY="260.0" prefHeight="50.0" prefWidth="320.0" promptText="密码" style="-fx-background-color: #ebeef1; -fx-background-radius: 20px;">
                    <font>
                        <Font name="Microsoft YaHei Bold" size="18.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="TEXT" />
                    </cursor>
                </PasswordField>
                <ImageView fitHeight="150.0" fitWidth="40.0" layoutX="437.0" layoutY="170.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/userName.png" />
                    </image>
                </ImageView>
                <ImageView fitHeight="150.0" fitWidth="45.0" layoutX="435.0" layoutY="262.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/userPassword.png" />
                    </image>
                </ImageView>
                <JFXButton fx:id="signUpButton" buttonType="RAISED" layoutX="548.0" layoutY="352.0" prefHeight="60.0" prefWidth="200.0" style="-fx-background-radius: 90px; -fx-background-color: #5178d9;" text="注册" textFill="WHITE">
                    <font>
                        <Font name="Microsoft YaHei Bold" size="15.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </JFXButton>
            <ImageView fitHeight="150.0" fitWidth="40.0" layoutX="437.0" layoutY="84.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/Email.png" />
               </image>
            </ImageView>
            </children>
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
        </Pane>
        <JFXButton fx:id="closeLoginButton" buttonType="RAISED" layoutX="875.0" layoutY="15.0" onAction="#close" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #fb4f4f; -fx-background-radius: 90px;" text="×" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Microsoft YaHei Bold" size="25.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="minimizeButton" layoutX="815.0" layoutY="15.0" onAction="#minimize" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #41caf6; -fx-background-radius: 90px;" text="-" textFill="WHITE">
            <font>
                <Font name="Microsoft YaHei Bold" size="25.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
      <JFXButton fx:id="backLoginButtion" buttonType="RAISED" layoutX="30.0" layoutY="20.0" onAction="#backButtion" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #adf19b; -fx-background-radius: 90px;" text="⬅" textFill="WHITE">
         <font>
            <Font name="Microsoft YaHei Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></JFXButton>
    </children>
</AnchorPane>
