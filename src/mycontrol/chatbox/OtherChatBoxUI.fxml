<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="rooHbox" prefWidth="600.0" type="HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints fx:id="colWidth" hgrow="SOMETIMES" prefWidth="40.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints fx:id="upRowHeight" prefHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints fx:id="downRowHeight"  prefHeight="40.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ImageView fx:id="otherHeadImage" fitHeight="40" fitWidth="40" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1" />
            <Label fx:id="nameLabel" text="Label">
               <font>
                  <Font name="Microsoft YaHei" size="12.0" />
               </font>
            </Label>
         </children>
      </GridPane>
      <Pane prefHeight="20.0" prefWidth="20.0" />
      <TextFlow fx:id="otherMessage" prefHeight="200.0" prefWidth="544.0" />
   </children>
</fx:root>
